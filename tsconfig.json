{
  "extends": "expo/tsconfig.base",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "compilerOptions": {
    "preserveConstEnums": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "$components": [
        "src/components/"
      ],
      "$components/*": [
        "src/components/*"
      ],
      "$screens": [
        "src/screens"
      ],
      "$screens/*": [
        "src/screens/*"
      ],
      "$utils": [
        "src/utils"
      ],
      "$utils/*": [
        "src/utils/*"
      ],
      "$xuder": [
        "src/xuder"
      ],
      "$xuder/*": [
        "src/xuder/*"
      ],
    },
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
}
