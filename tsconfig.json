{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "preserveConstEnums": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "$components": [
        "src/components/"
      ],
      "$components/*": [
        "src/components/*"
      ],
      "$pages": [
        "src/components/pages"
      ],
      "$pages/*": [
        "src/components/pages/*"
      ],
      "$utils": [
        "src/components/utils"
      ],
      "$utils/*": [
        "src/components/utils/*"
      ],
      "$xuder": [
        "src/xuder"
      ],
      "$xuder/*": [
        "src/xuder/*"
      ],
    },
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
}
